<header>
  
  <%= image_tag("logo.svg", width: 200) %>

  <h1>Courses</h1>

<ul>
  <% @courses.each do |course| %>
    <li>
     <a href="/courses/<%= course.id %>">
      <%= course.title %>
      </a>
    </li>
  <% end %>
</ul> 

<h1>Units</h1>

<ul>
  <% @units.each do |unit| %>
    <li>
     <a href="/units/<%= unit.id %>">
      <%= unit.title %>
      </a>
    </li>
  <% end %>
</ul> 

 


    <div class="text-center"><%= render 'courses/form' %> </div>
